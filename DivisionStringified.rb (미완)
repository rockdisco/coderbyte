def DivisionStringified(num1,num2)
  num = (num1.to_f/num2).round
  arr = num.to_s.split(//).to_a
  lth = arr.length
  rst = []
  
  arr.each_index do |i|
    if i-lth % 3 == 0
      rst << ","
      rst << arr[i]
    else
      rst << arr[i]
    end
  end
  
  return rst.join

end
DivisionStringified(STDIN.gets)

#num1, num2가 8자리 이상이면 안 됩니다... 왜죠...
